  {
    "name": "Scraping",
    "version": "1.0",
    "description": "An extension to make web scraping much easier",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "http://*/*",
        "https://*/*"

    ],
    "background": {
      "scripts": ["scraping.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {

      }
    },
    "manifest_version": 2

  }